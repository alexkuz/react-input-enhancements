{
  "name": "react-input-enhancements",
  "version": "0.4.12",
  "description": "Set of enhancements for input control (bootstrap-ready)",
  "scripts": {
    "build:lib": "NODE_ENV=production babel src --out-dir lib",
    "build:demo": "NODE_ENV=production webpack -p --optimize-dedupe",
    "stats": "webpack --profile --json > stats.json",
    "start": "webpack-dev-server",
    "lint": "eslint src",
    "version": "npm run build:demo && git add -A .",
    "postversion": "git push",
    "prepublish": "npm run build:lib"
  },
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/alexkuz/react-input-enhancements"
  },
  "keywords": [
    "react",
    "reactjs",
    "input",
    "autocomplete",
    "autosize",
    "typeahead",
    "mask",
    "dropdown",
    "combobox"
  ],
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.4.7",
    "babel-eslint": "^4.0.10",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.0.3",
    "babel-runtime": "^5.8.20",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.23.0",
    "eslint": "^1.2.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.2.3",
    "export-files-webpack-plugin": "0.0.1",
    "expose-loader": "^0.7.1",
    "html-webpack-plugin": "^1.6.1",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.2",
    "nyan-progress-webpack-plugin": "^1.1.4",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0",
    "react-bootstrap": "^0.28.1",
    "react-transform-hmr": "^1.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "dependencies": {
    "classnames": "^2.2.1",
    "css-vendor": "^0.3.1",
    "jss": "^3.2.0",
    "jss-nested": "^1.0.1",
    "jss-vendor-prefixer": "^1.0.1",
    "lodash.sortby": "^3.1.5",
    "moment": "^2.13.0",
    "react-date-picker": "^3.1.9",
    "react-pure-render": "^1.0.2"
  },
  "author": "Alexander <alexkuz@gmail.com> (http://kuzya.org/)",
  "license": "MIT"
}
